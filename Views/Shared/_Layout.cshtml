<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - StrongPack</title>
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css">
    <!-- Google Fonts -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap">
    <!-- Bootstrap core CSS -->
    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />*@
    <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.0/css/bootstrap.min.css" rel="stylesheet">
    <!-- Material Design Bootstrap -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.19.1/css/mdb.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <!-- Main navigation -->
    <!-- Navbar -->
    <nav class="navbar navbar-expand-xl navbar-dark fixed-top scrolling-navbar py-4 justify-content-around">
        <div class="container col-md-11 mx-auto">
            <a class="navbar-brand" href="#">
                <img class="" height="85" src="~/img/img-logo-strongpack.png" alt="Alternate Text" />
            </a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo02"
                    aria-controls="navbarTogglerDemo02" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse h3" id="navbarTogglerDemo02">
                <ul class="navbar-nav ml-auto smooth-scroll h-50">
                    <li class="nav-item">
                        <a class="nav-link" href="#intro">POLIBURBUJA</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#about">PELÍCULA STRECH</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#projects">RECICLADO</a>
                    </li>
                    <li class="nav-item mr-3">
                        <a class="nav-link" href="#gallery">CONTACTO</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- Navbar -->
    <!--Main layout-->
    @*<div class="main">*@
    @RenderBody()
    @*</div>*@
    <!--Main layout-->
    <!-- Footer -->
    <div class="bg-real-dark text-white py-5">
        <div class="row w-100 mx-0">
            <div class="col-md-5 txt-500">
                <div class="col-md-11 ml-auto">
                    <img class="" height="85" src="~/img/img-logo-strongpack.png" alt="Alternate Text" />
                    <br />
                    <div class="col-md-12">55 7592 0964</div>
                    <div class="col-md-12">55 7592 0965</div>
                    <br />
                    <div class="col-md-12">
                        Sur 23 No. 156, Col. La Purísima <br />
                        Alcaldía Iztapalapa, C.P. 09310 CDMX
                    </div>
                </div>
            </div>
            <div class="col-md-7 w-100 px-0">
                <div class="col-md-12 px-0">
                    <hr class="px-0" style="opacity:1; background-color:white; color:white; height:1rem;"/>
                </div>
                <div class="col-md-12 py-5">
                    <div class="text-center">
                        <!-- Facebook -->
                        <a class="">
                            <i class="fab fa-facebook-f white-text px-3 fa-2x"></i>
                        </a>
                        <!--Linkedin -->
                        <a class="">
                            <i class="fab fa-linkedin-in white-text px-3 fa-2x"></i>
                        </a>
                        <!--YouTube-->
                        <a class="">
                            <i class="fab fa-youtube white-text px-3 fa-2x"></i>
                        </a>
                    </div>
                </div>
                <div class="col-md-12 text-center txt-500">
                    <div>
                        Todos los derechos reservados Strong Pack @DateTime.Now.Year
                        <br />Diseño por "MC2 Arquitectura y diseño".
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Footer -->
    <!-- JQuery -->
    @*<script src="~/lib/jquery/dist/jquery.min.js"></script>*@
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <!-- Bootstrap -->
    @*<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>*@
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.4/umd/popper.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.0/js/bootstrap.min.js"></script>
    <!-- MDB core JavaScript -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.19.1/js/mdb.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
